}




.navbar {
	text-transform: uppercase;
	margin-top: -40px;
}

.navbar-inner {
	background: white;
}

.navbar li {
	font-size: 14px;
	font-weight: bold;
}

.navbar li a {
	color: #999999 !important;
}

.navbar li a.active {
	background: white !important;
