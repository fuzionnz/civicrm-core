	display: none;
}

#top #contact {
	padding: 15px 0 0px 0;
	min-height: 45px;
}

#top #search {
	padding: 15px 0 5px 0;
	min-height: 46px;
}

.activebutton {
	color: #7BCA7A !important;
	background-color: white !important;
}

.green-btn {
