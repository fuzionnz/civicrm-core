	display: block;
	float: left;
	padding: 8px 12px;
	font-size: 12px;
	font-weight: bold;
	border-radius: 4px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
}

.green-btn a {
	color: white;
}

.tool-item {
	display: inline-block;
	margin: 0 0 10px 20px;
}

#forum .tool-item {
	margin: 0;
}

#search .tool-item {
	margin: 0;
}

.green-btn.ico {
	padding: 0px;
	margin-right: 10px;
}

