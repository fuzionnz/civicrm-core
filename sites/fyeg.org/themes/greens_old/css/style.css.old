}


#green-box .footer {
	text-align: center;
	height: 22px;
}


#green-box .footer a {
	display: inline-block;
	color: white;
	background-color: #F3CC2F;
	
	padding: 10px;
	border: 2px solid #4BB749;
	
	border-radius: 5px;
	-moz-border-radius: 5px;
	-webkitborder-radius: 5px;
	
	
	text-decoration: none;
	font-weight: bold;
}



#newsletter form {
	margin: 0;
}

#newsletter .header{
	float: left;
}


#newsletter .content{
	float: left;
	clear: right;
}


#newsletter .icon {
	float: left;
	margin-bottom: 15px;
	margin-right: 22px;
}

#newsletter .icon img {
	background-color: #4BB749;
	border-radius: 50%;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
}




#newsletter .title {
	float: left;
