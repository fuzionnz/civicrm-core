}

.box.events .title {
	margin-left: 5px;
}

.box.events .arrows{
	float: left;
	padding-left: 3%;
}

.box.events .months{
	float: right;
	background-color: #4BB749;
	color: #ABDEAA;
	width: 77%;
	border-radius: 6px;
	-moz-border-radius: 6px;
	-webkit-border-radius: 6px;		
}

.box.events .month {
	float: left;
	width: 133px;
	line-height: 40px;
	text-align: center;
	font-weight: bold;
}

.box.events .month.swiper-slide-active {
	color: white
}

.box.events .month.active{
