	display: inline-block;
}


#top #forum {
	padding: 15px 0 5px 0;
}

#top #tools .signup {
	background-color: #EEEEEE;
	color: #666;
	font-weight: bold;
	padding: 6px 10px 7px;
	border-radius: 4px;
	display: block;
	float: left;
	margin-top: 1px;
}

#top #tools .signup:hover {
	background: white;
}

